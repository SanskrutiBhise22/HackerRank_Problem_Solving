import java.io.*;
import java.math.*;
import java.security.*;
import java.text.*;
import java.util.*;
import java.util.concurrent.*;
import java.util.regex.*;

class Result {

    public static List<Integer> rotateLeft(int d, List<Integer> arr) {
        for(int i=0;i<d;i++){
            arr.add(arr.get(0));
            arr.remove(0);
        }  
        return arr;     

    }

}
