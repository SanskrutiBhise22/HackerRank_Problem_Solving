import java.io.*;
import java.math.*;
import java.security.*;
import java.text.*;
import java.util.*;
import java.util.concurrent.*;
import java.util.regex.*;

class Result {

    public static int introTutorial(int V, List<Integer> arr) {
      for(int i=0;i<arr.size();i++){
          if(arr.get(i)==V){
              return i;
          }
      }
      return -1;
    }

}
time complexity:O(n)
