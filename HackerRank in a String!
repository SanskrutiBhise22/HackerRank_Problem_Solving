class Result {
    public static String hackerrankInString(String s) {
     String str="hackerrank";
     int j=0;
     for(int i=0;i<s.length();i++){
         if(j<10 && s.charAt(i)==str.charAt(j)){
             j++;
         }
     }
     if(j!=10){
        return "NO";
     }
      return "YES";
    }
}
time complexity = O(n) 
space complexity = O(1)
